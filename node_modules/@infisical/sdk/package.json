{
  "name": "@infisical/sdk",
  "version": "2.1.8",
  "license": "SEE LICENSE IN LICENSE",
  "author": "Infisical <team@infisical.com> (https://infisical.com)",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/infisical/sdk"
  },
  "files": [
    "binding.js",
    "binding.d.ts",
    "./lib/",
    "./lib/infisical_client/"
  ],
  "napi": {
    "name": "infisical",
    "defaults": true,
    "triples": {
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "i686-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "arm-linux-androideabi",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "scripts": {
    "build": "yarn --cwd ../../crates/infisical-napi build",
    "post-build": "npm run move && npm run format-binding",
    "move": "npm run move:bindings && npm run move:binaries",
    "move:bindings": "mv -v ../../crates/infisical-napi/binding.* ./",
    "artifacts": "napi artifacts",
    "prepublishOnly": "napi prepublish --skip-gh-release",
    "move:binaries": "mv -v ../../crates/infisical-napi/*.node ./",
    "copy:artifacts": "cp ./artifacts/schemas.ts ./src/infisical_client/schemas.ts && cp ./artifacts/binding.* ./",
    "tsc": "tsc",
    "format-binding": "node ./format-binding.js",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.13.2",
    "ts-node": "10.9.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "tinyify": "4.0.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "optionalDependencies": {
    "@infisical/sdk-win32-x64-msvc": "2.1.8",
    "@infisical/sdk-darwin-x64": "2.1.8",
    "@infisical/sdk-linux-x64-gnu": "2.1.8",
    "@infisical/sdk-linux-x64-musl": "2.1.8",
    "@infisical/sdk-linux-arm64-gnu": "2.1.8",
    "@infisical/sdk-win32-ia32-msvc": "2.1.8",
    "@infisical/sdk-linux-arm-gnueabihf": "2.1.8",
    "@infisical/sdk-darwin-arm64": "2.1.8",
    "@infisical/sdk-android-arm64": "2.1.8",
    "@infisical/sdk-android-arm-eabi": "2.1.8",
    "@infisical/sdk-linux-arm64-musl": "2.1.8",
    "@infisical/sdk-win32-arm64-msvc": "2.1.8"
  }
}